     "version": "1.1.0",
    "autoload": {
        "psr-4": {
            "BblStudio\\BblStudio\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Elidi Naili",
            "email": "nailiyassin2@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BblStudio\\BblStudio\\BblStudioServiceProvider"
            ]
        }
    }
}
